{
	"name": "gettext-parser-next",
	"description": "Gettext parser fork, with modern js and types, this library is used to parse and compile gettext po and mo files to/from json",
	"version": "1.0.0",
	"author": "erikyo <https://github.com/erikyo>",
	"homepage": "https://github.com/erikyo/gettext-parser-next",
	"repository": {
		"type": "git",
		"url": "https://github.com/erikyo/gettext-parser-next.git"
	},
	"bugs": {
		"url": "https://github.com/erikyo/gettext-parser-next/issues"
	},
	"type": "module",
	"engines": {
		"node": ">=14"
	},
	"files": ["lib", "tests", "*.json", "*.md"],
	"scripts": {
		"lint": "npx @biomejs/biome check --apply src test",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"build": "node build.mjs && npm run types",
		"watch": "npx esbuild ./src/index.ts --bundle --outfile=lib/esm/index.js --watch",
		"types": "tsc --emitDeclarationOnly"
	},
	"main": "./lib/esm/index.js",
	"module": "./lib/esm/index.js",
	"require": "./lib/cjs/index.js",
	"types": "./lib/@types/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"dependencies": {
		"content-type": "^1.0.5",
		"iconv-lite": "^0.6.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.1",
		"@types/content-type": "^1.1.8",
		"@types/node": "^20.12.7",
		"@vitest/coverage-v8": "^1.5.2",
		"esbuild": "^0.20.2",
		"typescript": "^5.4.4",
		"vitest": "^1.5.2"
	},
	"keywords": ["i18n", "l10n", "gettext", "mo", "po"]
}
